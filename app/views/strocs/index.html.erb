<p id="notice"><%= notice %></p>

<h1>Listing Strocs</h1>

<table>
  <thead>
    <tr>
      <th> name</th>
      <th>Symbol</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @strocs.each do |stroc| %>
      <tr>
        <td><%= stroc. name %></td>
        <td><%= stroc.symbol %></td>
        <td><%= link_to 'Show', stroc %></td>
        <td><%= link_to 'Edit', edit_stroc_path(stroc) %></td>
        <td><%= link_to 'Destroy', stroc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stroc', new_stroc_path %>
